
def on_reset($scratch_space: [Int], test: Int) {

    ui_set_text("description", `Multiply two numbers`)

}

def get_input($scratch_space: [Int], tick: Int, $inputs: [Int]) {

    var a = random(256)
    var b = random(256)

    inputs[0] = a
    inputs[1] = b

    //ui_set_text("description", `What is {a} * {b}?`)

}

def check_output($scratch_space: [Int], tick: Int, inputs: [Int], outputs: [Int]) TestResult {

    let result = (inputs[0] * inputs[1]) & 0xff

    if outputs[0] != result {

        set_error(`The lower byte of {inputs[0]} * {inputs[1]} is {result}`)
        return fail

    }

    if tick == 0xffff {
        return win
    }

}




